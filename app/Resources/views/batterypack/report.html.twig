{% extends 'base.html.twig' %}
{% block body %}
	<h3>Battery summary</h3>
	<table border="1" cellpadding="5" cellspacing="5">
		<thead>
				<th style="text-align: center">batteryType</th>
				<th style="text-align: center">counter</th>
		</thead>
		<tbody>
			{% for row in data %}
			<tr>
				<td style="text-align: left">{{ row.batteryType }}</td>
				<td style="text-align: right">{{ row.counter }}</td>
			</tr>
			{% endfor %}
		</tbody>
	</table>
{% endblock %}